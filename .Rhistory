library(jsonlite)
# Specify your file path
file_path <- "datan3.txt"
# Open the file connection
con <- file(file_path, "r")
# Initialize an empty list to store parsed JSON data
json_data <- list()
# Read the file line by line and parse each line
while (length(line <- readLines(con, n = 1)) > 0) {
parsed_json <- fromJSON(line, flatten = TRUE)
json_data <- c(json_data, parsed_json)
}
# Close the file connection
close(con)
# Convert the list to a data frame
df <- as.data.frame(do.call(rbind, json_data))
View(df)
